{
	"$schema": "https://schema.tauri.app/config/2",
	"productName": "differ",
	"version": "0.1.0",
	"identifier": "com.dell.differ",
	"build": {
		"beforeDevCommand": "pnpm run dev",
		"devUrl": "http://localhost:1420",
		"beforeBuildCommand": "pnpm run build",
		"frontendDist": "../dist"
	},
	"app": {
		"windows": [
			{
				"title": "Differ",
				"width": 800,
				"height": 600,
				"minWidth": 843,
				"minHeight": 570,
				"decorations": false,
				"shadow": true
			}
		],
		"security": {
			"csp": null
		}
	},
	"bundle": {
		"active": true,
		"targets": "all",
		"icon": [
			"icons/32x32.png",
			"icons/128x128.png",
			"icons/128x128@2x.png",
			"icons/icon.icns",
			"icons/icon.ico"
		]
	}
}
